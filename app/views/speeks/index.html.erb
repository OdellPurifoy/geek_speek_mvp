<p id="notice"><%= notice %></p>

<h1>Speeks</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @speeks.each do |speek| %>
      <tr>
        <td><%= speek.content %></td>
        <td><%= speek.user_id %></td>
        <td><%= link_to 'Show', speek %></td>
        <td><%= link_to 'Edit', edit_speek_path(speek) %></td>
        <td><%= link_to 'Destroy', speek, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Speek', new_speek_path %>
